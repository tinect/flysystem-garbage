{
  "name": "tinect/flysystem-garbage",
  "description": "Flysystem Adapter to copy files into garbage folder when specific actions are taken",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "tinect"
    }
  ],
  "require": {
    "php": "^8.1",
    "league/flysystem": "^3.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "league/flysystem-adapter-test-utilities": "^3.10.0",
    "league/flysystem-memory": "^3.10.0",
    "infection/infection": "^0.27.0"
  },
  "autoload": {
    "psr-4": {
      "Tinect\\Flysystem\\Garbage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tinect\\Flysystem\\Garbage\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "scripts": {
    "phpunit": "vendor/bin/phpunit",
    "infection": "vendor/bin/infection --min-covered-msi=87 --threads=4 --show-mutations"
  }
}
