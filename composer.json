{
  "name": "tinect/flysystem-garbage",
  "description": "Flysystem Adapter to copy files into garbage folder when specific actions are taken",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "tinect"
    }
  ],
  "require": {
    "php": "^8.1",
    "league/flysystem": "^3.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "league/flysystem-adapter-test-utilities": "^3.10.0"
  },
  "autoload": {
    "psr-4": {
      "Tinect\\Flysystem\\Garbage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tinect\\Flysystem\\Garbage\\Tests\\": "tests"
    }
  }
}